{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "config": {"view": {"continuousWidth": 600}},
  "vconcat": [
    {
      "width": 600,
      "height": 420,
      "title": {
        "text": "",
        "anchor": "start"
      },
      "data": {
        "values": []
      },
      "encoding": {
        "y": {
          "field": "Entity",
          "type": "ordinal",
          "sort": null,
          "axis": {"grid": false, "labelFontSize": 17, "labelLimit": 0}
        },
        "x": {
          "field": "value",
          "type": "quantitative",
          "axis": null
        },
        "opacity": {"value": 1}
      },
      "layer": [
        {
          "mark": {"type": "bar"},
          "encoding": {
            "color": {
              "condition":[
                {"test": "datum.Entity === 'none'", "value": "#d8a27d"}
              ]
              ,
              "value": "#949d48"
            },
            "opacity": {
              "condition": [
                {"test": "datum.Entity === 'none'", "value": 1}
              ],
              "value": 1
            },
            "size": {
              "condition": [
                {"test": "datum.Entity === 'none'", "value": 30}
              ],
              "value": 40
            }
          }
        },
        {
          "mark": {
            "type": "text",
            "dx": 0,
            "dy": 0,
            "xOffset": 12,
            "yOffset": 0,
            "angle": 0,
            "align": "left",
            "baseline": "middle",
            "font": "serif",
            "fontSize": 24,
            "fontWeight": "normal",
            "opacity": 1
          },
          "encoding": {
            "text": {"field": "value", "type": "quantitative"},
            "color": {"value": "#000000"},
            "x": {"field": "value", "type": "quantitative"}
          }
        }
      ]
    }
  ],
  "name": "template"
}